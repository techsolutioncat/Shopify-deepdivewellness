    "85ab6184-0ddf-4927-8d0d-8470b8023e94": {
      "type": "faq",
      "blocks": {
        "eb12c63e-cc22-4cfa-b804-b69bd5b31403": {
          "type": "faq",
          "settings": {
            "question": "What are the benefits of cold plunge bathing?",
            "font_size": "h3",
            "answer": "Cold plunge bathing has been shown to provide benefits such as reduced pain, improved circulation, reduced stress, increased mental clarity, and improved immune function.",
            "color_scheme": "accent-2"
          }
        },
        "3aba0ca7-3e62-43d4-9a1c-ec9d959f6359": {
          "type": "faq",
          "settings": {
            "question": "Can I use a cold plunge bath outdoors?",
            "font_size": "h3",
            "answer": "Yes, you can use a cold plunge bath outdoors, but it is important to protect the tub from weather elements",
            "color_scheme": "accent-2"
          }
        },
        "b4eb9ab2-ceeb-4c75-bd9f-ab681149041b": {
          "type": "faq",
          "settings": {
            "question": "How much ice should I add?",
            "font_size": "h3",
            "answer": "You'll need to place 20–30 pounds of ice, this ratio will bring the water temperature down to around 45 degrees Fahrenheit.",
            "color_scheme": "accent-2"
          }
        },
        "9baa7296-3733-4f25-9c33-b5b0ceae5505": {
          "type": "faq",
          "settings": {
            "question": "How to keep the water clean?",
            "font_size": "h3",
            "answer": "To maintain the cleanliness of your plunge, we suggest keeping our FREE complimentary lid on when not in use. It is also recommended to change the water once a week. When you drain the water, washing the inside surfaces with warm soapy water will help extend the life of your next fill.",
            "color_scheme": "accent-2"
          }
        },
        "58c0f2f4-01e3-435f-ac11-5ee715201079": {
          "type": "faq",
          "settings": {
            "question": "What temperature should the water be for a cold plunge bath?",
            "font_size": "h3",
            "answer": "The temperature of the water for a cold plunge bath typically ranges from 40-50°F.",
            "color_scheme": "accent-2"
          }
        },
        "de76124e-85a4-46c7-a231-e5ce90173cd3": {
          "type": "faq",
          "settings": {
            "question": "How long should I stay in the cold plunge bath?",
            "font_size": "h3",
            "answer": "The length of time you should stay in the cold plunge bath will depend on your individual tolerance. It is recommended to start with short durations (1-3 minutes) and gradually increase as your body becomes accustomed to the therapy.",
            "color_scheme": "accent-2"
          }
        },
        "bf532df7-62ef-4f96-b8c1-4cd293a60317": {
          "type": "faq",
          "settings": {
            "question": "Can I use a cold plunge bath if I have sensitive skin?",
            "font_size": "h3",
            "answer": "Cold water therapy may not be appropriate for individuals with sensitive skin. It is always best to consult with a healthcare professional to determine if this therapy is right for you.",
            "color_scheme": "accent-2"
          }
        },
        "e4f576d7-32e1-45c0-bd0d-ccbd78c9c5ff": {
          "type": "faq",
          "settings": {
            "question": "Can I use a cold plunge bath outdoors?",
            "font_size": "h3",
            "answer": "Yes, you can use a cold plunge bath outdoors, but it is important to protect the tub from weather elements",
            "color_scheme": "accent-2"
          }
        },
        "fd701979-31e4-4f46-b79a-36bd566e4b51": {
          "type": "faq",
          "settings": {
            "question": "How do I drain the plunge bath?",
            "font_size": "h3",
            "answer": "Deepdive Wellness plunge has a tap located at the bottom for easy draining.",
            "color_scheme": "accent-2"
          }
        },
        "6e5203eb-e954-4cc5-9b50-94b8f0ded4d2": {
          "type": "faq",
          "settings": {
            "question": "Is the plunge bath suitable for people over 6 feet tall?",
            "font_size": "h3",
            "answer": "Our plunge is suitable for heights up to 2.1 m (6′9 foot)",
            "color_scheme": "accent-2"
          }
        }
      },
