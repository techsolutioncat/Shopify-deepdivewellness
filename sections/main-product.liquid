        "@context": "http://schema.org/",
        "@type": "Product",
        "name": {{ product.title | json }},
        "url": {{ request.origin | append: product.url | json }},
        {% if seo_media -%}
            "image": [
            {{ seo_media | image_url: width: seo_media.preview_image.width | prepend: "https:" | json }}
            ],
        {%- endif %}
        "description": {{ product.description | strip_html | json }},
        {% if product.selected_or_first_available_variant.sku != blank -%}
            "sku": {{ product.selected_or_first_available_variant.sku | json }},
        {%- endif %}
        "brand": {
            "@type": "Brand",
            "name": {{ product.vendor | json }}
        },
        "offers": [
            {%- for variant in product.variants -%}
            {
                "@type" : "Offer",
                {%- if variant.sku != blank -%}
                "sku": {{ variant.sku | json }},
                {%- endif -%}
                {%- if variant.barcode.size == 12 -%}
                "gtin12": {{ variant.barcode }},
                {%- endif -%}
                {%- if variant.barcode.size == 13 -%}
                "gtin13": {{ variant.barcode }},
                {%- endif -%}
                {%- if variant.barcode.size == 14 -%}
                "gtin14": {{ variant.barcode }},
                {%- endif -%}
                "availability" : "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
                "price" : {{ variant.price | divided_by: 100.00 | json }},
                "priceCurrency" : {{ cart.currency.iso_code | json }},
                "url" : {{ request.origin | append: variant.url | json }}
            }{% unless forloop.last %},{% endunless %}
            {%- endfor -%}
        ]
        }
    </script>
</section>

<script>
  $('#shopify-section-{{ section.id }} .subscription-button').on('click', function() {
    const quantity = $(this).find('.subscription').data('quantity');
    $('#shopify-section-{{ section.id }} .subscription-button').removeClass('checked');
    $(this).addClass('checked');
    $('#shopify-section-{{ section.id }} input.quantity__input').val(quantity);
    {% comment %}
    $('#shopify-section-{{ section.id }} form#checkout_form input.quantity').val(quantity);variant-radios
    {% endcomment %}

    $('#shopify-section-{{ section.id }} form[data-type=add-to-cart-form]').submit();
  });

  $('#shopify-section-{{ section.id }} button#button_checkout').on('click', function() {
    {% comment %} var q = $('#shopify-section-{{ section.id }} input.quantity__input').val();
    $('#shopify-section-{{ section.id }} form#checkout_form input.quantity').val(q); {% endcomment %}
    $('#shopify-section-{{ section.id }} form[data-type=add-to-cart-form]').submit();
  });

  $('#shopify-section-{{ section.id }} .offer-button a').on('click', function() {
    $('#shopify-section-{{ section.id }} form#checkout_form input.quantity').val(2);
    $('#shopify-section-{{ section.id }} button#button_checkout').trigger( "click" );
  });
</script>

