<script>
  $(document).ready(function() {
    $('#shopify-section-{{section.id}} .pack-group .pack-item').on('click', function() {
      var q = $(this).data('quantity');
      $('#shopify-section-{{section.id}} .pack-group .pack-item').each(function() {
        $(this).removeClass('active');
      });
      $(this).addClass('active');
      $('#shopify-section-{{section.id}} .product-form__quantity input.quantity__input').val(q);
    });
  });
</script>